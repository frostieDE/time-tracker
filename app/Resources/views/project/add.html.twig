{% extends "main.html.twig" %}

{% block title %}{{ 'project.new.header'|trans }}{% endblock %}

{% block content %}
    <h3><i class="fa fa-plus"></i> {{ 'project.new.header'|trans }}</h3>

    {{ form_start(form) }}
    {{ form_errors(form) }}
    {{ form_widget(form) }}

    <div class="form-buttons">
        <button class="btn btn-primary">
            <i class="fa fa-plus"></i> {{ 'project.new.submit'|trans }}
        </button>
    </div>
    {{ form_end(form) }}
{% endblock %}